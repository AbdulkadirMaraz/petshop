<!-- Product Categories List Section Start -->
<div class="product-categories-section sisf-extended-grid sisf-extended-grid--right section">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-md-9">
                <!-- Section Title Start -->
                <div class="sisf-sis-section-title section-title wow fadeInDown mb-0">
                    <h2 class="sisf-m-title mb-0">
                        Öne Çıkan Ürünler <i class="fa fa-paw"> </i>
                    </h2>
                </div>
                <!-- Section Title End -->
            </div>
            <div class="col-md-3">
                <div class="sisf-m-button text-end my-3">
                    <a href="/CustomerProduct/Index" class="btn-default sisf-hover-reveal">Daha Fazla Görüntüle</a>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="product-list-slider sisf-product-list sisf-layout--standard wow fadeInUp">
                <div class="swiper d-flex justify-content-center">
                    <div class="swiper-wrapper">

                        @foreach (var category in Model)
                        {
                            @foreach (var item in category.Products)
                            {
                                <div class="swiper-slide">
                                    <div class="sisf-e-inner">
                                        <div class="product">
                                            <div class="sisf-product-inner sisf-e-inner">
                                                <!-- Product Image Start -->
                                                <div class="sisf-product-image">
                                                    <a href="/ProductDetails/Index/@item.Id">
                                                        <figure>
                                                            <img style="height:250px" src="@item.ImageUrl" class="image-fluid" alt="Pawly">
                                                        </figure>
                                                    </a>
                                                    <div class="heart-icon">
                                                        <a href="#">
                                                            <i class="fa-regular fa-heart"></i>
                                                        </a>
                                                    </div>
                                                    <div class="sisf-m-button cart-icon-button text-center">
                                                        <a href="/CustomerCart/Index" class="product_type_simple btn-default sisf-layout-filled add_to_cart_button">
                                                            <span><i class="fa-solid fa-cart-shopping me-2"></i> Sepete Ekle</span>
                                                        </a>
                                                    </div>
                                                </div>
                                                <!-- Product Image End -->
                                                <!-- Product Content Start -->
                                                <div class="sisf-e-product-content mb-0">
                                                    <div class="sisf-e-ratings sisf-m mt-3">
                                                        <div class="sisf-m-inner">
                                                            <div class="sisf-m-star sisf--initial">
                                                                <i class="fa-solid fa-star"></i>
                                                                <i class="fa-solid fa-star"></i>
                                                                <i class="fa-solid fa-star"></i>
                                                                <i class="fa-solid fa-star"></i>
                                                                <i class="fa-solid fa-star"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <h5 class="sisf-e-product-title sisf-e-title entry-title mt-3">
                                                        <a class="sisf-e-product-title-link shop-product" href="/ProductDetails/Index">
                                                            @item.Name
                                                        </a>
                                                    </h5>
                                                    <div class="sisf-product-price price mb-0">
                                                        <span class="product-price-amount">
                                                            <sup class="product-price-currencysymbol">$</sup>@item.Price
                                                        </span>
                                                    </div>
                                                </div>
                                                <!-- Product Content End -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }

                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Product Categories List Section End -->
<!-- Shop by Categories Section Start -->
<div class="shop-categories-section section">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <!-- Section Title Start -->
                <div class="sisf-sis-section-title section-title text-center wow fadeInDown">
                    <h2 class="sisf-m-title text-white">
                        Kategorilere göre alışveriş yapın <i class="fa fa-paw"> </i>
                    </h2>
                </div>
                <!-- Section Title End -->
            </div>
        </div>
        <div class="row">
            <div class="sisf-sis-category-slider">
                <div class="swiper">
                    <div class="swiper-wrapper">
                        @foreach (var item in Model)
                        {
                            <div class="swiper-slide">
                                <div class="sisf-e-inner">
                                    <div class="sisf-e-content-holder text-center">
                                        <div class="sisf-e-title sisf-item-content">
                                            <h4 class="sisf-e-title">
                                                <a href="/CustomerProduct/Index?categoryId=@item.CategoryId">@item.CategoryName</a>

                                            </h4>
                                        </div>
                                        <div class="sisf-category-button sisf-item-content mb-3">
                                            <a class="sisf-sis-button sisf-text-underline sisf-underline--left" href="/CustomerProduct/Index?categoryId=@item.CategoryId">

                                                <span class="sisf-m-text">Şimdi al </span>
                                            </a>
                                        </div>
                                    </div>
                                    <div class="sisf-e-images-holder">
                                        <div class="sisf-e-main-image text-center">
                                            <a href="/CustomerProduct/Index?categoryId=@item.CategoryId" />
                                            <img style="height:120px" src="@item.CategoryImageUrl" alt="Pawly">

                                        </div>
                                    </div>
                                </div>
                            </div>

                        }

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
