@using EntityLayer.Concrete
@model List<CustomerPetshop.Models.ViewModels.CategoryWithProductsViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="hero hero-slider position-relative">
    <div class="hero-slider-layout">
        <div class="swiper">
            <div class="swiper-wrapper">
                <!-- Hero Slide Start -->
                @{

                    var last = ViewBag.LastProducts as IEnumerable<EntityLayer.Concrete.Product>;
                }

                @if (last.Any())
                {

                    <div class="swiper-slide">
                        <div class="hero-slide hero-video">
                            <div class="hero-bg-video">
                                <!-- Video Start -->
                                <video autoplay="" muted="" loop="" id="mynewVideo">
                                    <source src="https://pawly.wpthemeverse.com/wp-content/uploads/2024/10/video-5-1.mp4" type="video/mp4">
                                </video>
                                <!-- Video End -->
                            </div>
                            <!-- Slider Content Start -->
                            <div class="container">
                                <div class="row align-items-center">
                                    <div class="col-12">
                                        <!-- Hero Content Start -->
                                        <div class="hero-content">
                                            <!-- Hero Title Start -->
                                            <div class="section-title">
                                                <h1 class="text-anime-style-2 text-center text-heading wow fadeInUp" data-cursor="-opaque">Elit Evcil Hayvan Malzemeleri</h1>
                                                <div class="row">
                                                    @foreach (var item in last)
                                                    {
                                                        <div class="col-4">

                                                            <div class="shop-item my-4">
                                                                <figure>
                                                                    <img src="@item.ImageUrl" class="half-rotate" alt="Pawly">
                                                                </figure>
                                                                <div class="shop-item--content">
                                                                    <h3 class="title">@item.Name</h3>
                                                                    <span>@item.Description</span>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    }
                                                </div>
                                                <p class="text-center ms-auto me-auto">Evcil hayvanlarla ilgili her şey için tek adresiniz! İster gururlu bir köpek sahibi, ister kedi sever, ister küçük hayvanlara bakıyor olun</p>
                                            </div>
                                            <!-- Hero Title End -->
                                            <!-- Hero Content Body Start -->
                                            <div class="sisf-m-button text-center hero-content-body wow fadeInUp">
                                                <a href="/CustomerProduct/Index" class="btn-slider">
                                                    <span>Alışveriş</span>
                                                </a>
                                            </div>
                                            <!-- Hero Content Body End -->
                                        </div>
                                        <!-- Hero Content End -->
                                    </div>
                                </div>
                            </div>
                            <!-- Slider Content End -->
                        </div>
                    </div>


